{
	"name": "pdffiller/laravel-resque",
	"version": "1.4.0",
	"description": "Laravel Resque connector for Queue Support Laravel 5.3.",
	"keywords": ["laravel", "resque", "queue", "pdffiller"],
	"license": "MIT",
	"authors": [
		{
			"name": "Viktor Nikitash",
			"email": "nikitashvictor@gmail.com"
		}
	],
	"require": {
		"php": ">=5.6.4",
		"illuminate/config": "5.3.x",
		"illuminate/queue": "5.3.x",
		"pdffiller/php-resque": "^1.5"
	},
	"suggest": {
		"pdffiller/php-resque-scheduler": "Enables the use of the ResqueScheduler functionality."
	},
	"autoload": {
		"psr-0": {
			"Awellis13\\Resque": "src/"
		}
	},
	"repositories": {
		"php-resque": {
			"type": "vcs",
			"url": "git@github.com:pdffiller/php-resque.git"
		},
		"php-resque-scheduler": {
			"type": "vcs",
			"url": "git@github.com:pdffiller/php-resque-scheduler.git"
		}
	},
	"minimum-stability": "stable"
}
